<?xml version="1.0" encoding="utf-8" ?>
<Config>
  <!-- Definitions of data sources; (required) -->
  <Sources>
    <!-- Definition of a data source connecting to an Openlink Virtuoso database instance using JDBC -->
    <DataSource type="virtuoso">
      <Param name="host" value="localhost" />
      <Param name="port" value="1111" />
      <Param name="username" value="dba" />
      <Param name="password" value="dba" />
    </DataSource>
  </Sources>
  
  <!-- Definition of where the output shall be written (required) -->
  <Outputs>
    <Output type="file">
      <Param name="path" value="out.n3"/>
      <Param name="format" value="ntriples"/>
    </Output>
  </Outputs>
</Config>
