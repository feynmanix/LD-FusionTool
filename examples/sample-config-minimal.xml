<?xml version="1.0" encoding="utf-8" ?>
<Config>
  <!-- Definitions of data sources; (required) -->
  <Sources>
    <DataSource type="file">
      <Param name="path" value="data.rdf" />
    </DataSource>
  </Sources>
  
  <!-- Definition of where the output shall be written (required) -->
  <Outputs>
    <Output type="file">
      <Param name="path" value="out.n3"/>
      <Param name="format" value="ntriples"/>
    </Output>
  </Outputs>
</Config>
